---
import type { Layout } from '../../libs/layout'
import Skippy from './Skippy.astro'
import Symbols from '../icons/Symbols.astro'
import Navigation from './Navigation.astro'
import Debug from '../Debug.astro'

interface Props {
  layout: Layout
  title: string
}

const { layout, title } = Astro.props
---

<Skippy layout={layout} />
<Symbols />

<!-- TODO(HiDeoo) debug section to remove -->
<Debug />
<!-- TODO(HiDeoo) end of debug section to remove -->

<Navigation layout={layout} title={title} />
